{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "hugo",
  "frontMatter.content.publicFolder": "static",
  "frontMatter.content.pageFolders": [
    {
      "title": "posts",
      "path": "[[workspace]]/content/posts"
    }
  ],
  "frontMatter.taxonomy.tags": [
    "build log",
    "buildlog",
    "claviers",
    "config",
    "git",
    "keyboard"
  ],
  "frontMatter.taxonomy.categories": [
    "claviers",
    "keyboard",
    "tech"
  ],
  "frontMatter.taxonomy.customTaxonomy": [
    {
      "id": "series",
      "options": [
        "La th√©orie des claviers",
        "Keyboard 101"
      ]
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "default",
      "pageBundle": false,
      "fields": [
        {
          "title": "Title",
          "name": "title",
          "type": "string"
        },
        {
          "title": "Content preview",
          "name": "cover",
          "type": "image"
        },
        {
          "title": "author",
          "name": "author",
          "type": "string"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string"
        },
        {
          "title": "Publishing date",
          "name": "date",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "name": "divider",
          "type": "divider"
        },
        {
          "title": "Reading Time",
          "name": "readingTime",
          "type": "boolean"
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "draft"
        },
        {
          "title": "Toc",
          "name": "toc",
          "type": "boolean"
        },
        {
          "name": "divider",
          "type": "divider"
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "series",
          "name": "series",
          "type": "taxonomy",
          "taxonomyId": "series"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "type",
          "name": "type",
          "type": "choice",
          "choices": ["post"]
        }
      ]
    }
  ]
}